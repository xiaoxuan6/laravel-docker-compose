{
  "name": "james.xue/laravel-docker-compose",
  "description": "Laravel docker-compose",
  "keywords": [
    "docker",
    "laravel",
    "docker-compose",
    "laravel-docker-compose"
  ],
  "homepage": "https://github.com/xiaoxuan6/laravel-docker-compose.git",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Vinhson\\LaravelDockerCompose\\": "src/"
    }
  },
  "authors": [
    {
      "name": "xiaoxuan6",
      "email": "1527736751@qq.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.3",
    "laravel/framework": "^6.0||^7.0||^8.0||^9.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vinhson\\LaravelDockerCompose\\LaravelDockerComposeServiceProvider"
      ]
    },
    "branch-alias": {}
  },
  "minimum-stability": "dev",
  "scripts": {
    "fix-style": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --ansi"
  },
  "scripts-descriptions": {
    "fix-style": "Run style checks and fix violations."
  },
  "config": {
    "sort-packages": true
  }
}
